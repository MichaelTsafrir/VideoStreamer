{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Node",
			"type": "node",
			"request": "launch",
			"args": ["${workspaceRoot}/server/index.ts"],
			"runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
			"sourceMaps": true,
			"cwd": "${workspaceRoot}",
			"protocol": "inspector",
		},
		{
			"name": "React",
			"type": "chrome",
			"request": "launch",
			"url": "http://localhost:3000",
			"webRoot": "${workspaceRoot}/src",
			"sourceMapPathOverrides": {
				"webpack:///src/*": "${webRoot}/*"
			}
		}
	]
}